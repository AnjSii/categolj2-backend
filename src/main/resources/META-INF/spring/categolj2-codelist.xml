<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="CL_FORMAT" class="am.ik.categolj2.infra.codelist.EnumCodeList">
        <property name="enumClass"
                  value="am.ik.categolj2.domain.model.EntryFormat"/>
    </bean>

    <bean id="CL_LOGGER_LEVEL" class="org.terasoluna.gfw.common.codelist.SimpleMapCodeList">
        <property name="map">
            <map>
                <entry key="OFF" value="OFF"/>
                <entry key="ERROR" value="ERROR"/>
                <entry key="WARN" value="WARN"/>
                <entry key="INFO" value="INFO"/>
                <entry key="DEBUG" value="DEBUG"/>
                <entry key="TRACE" value="TRACE"/>
                <entry key="ALL" value="ALL"/>
            </map>
        </property>
    </bean>
</beans>
